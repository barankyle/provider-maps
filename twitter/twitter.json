{
  "version": "1.0",

  "name": "Twitter",
  "description": "Online social networking service.",
  "url": "https://api.twitter.com",
  "tags": ["social"],

  "auth": {
    "type": "oauth1",
    "version": "1.0a",
    "signature_method": "HMAC-SHA1",
    "redirect_url": "https://twitter.com",
    "authorization_url": "https://api.twitter.com/oauth/authenticate",

    "request_token": "https://api.twitter.com/oauth/request_token",
    "access_token": "https://api.twitter.com/oauth/access_token",

    "auth_key": null,
    "auth_secret": null
  },

  "meta": {
    "uniqueness_location": "id_str",
    "default_name_location": "screen_name",
    "endpoint": {
      "method": "GET",
      "parameters": {
        "include_entities": {
          "description": "Whether or not to include the user's last tweet",
          "value": false
        }
      },
      "route": {
        "data": "",
        "path": "1.1/account/verify_credentials.json"
      },
      "model": {
        "key": "id_str",
        "fields": {
          "id_str": "string",
          "name": "string",
          "screen_name": "string"
        }
      }
    }
  },

  "endpoints": {
    "DirectMessagesReceived": {
      "method": "GET",
      "parameters": {
        "count": {
          "description": "Pagination counts",
          "value": 200
        },
        "since_id": {
          "description": "Most recent direct message that was saved in BitScoop",
          "value": "$connection.endpoint_data.DirectMessagesReceived.since_id"
        },
        "max_id": {
          "description": "Highest ID of next page call",
          "value": "$connection.endpoint_data.DirectMessagesReceived.max_id"
        }
      },
      "routes": {
        "single": null,
        "collection": {
          "data": "",
          "path": "1.1/direct_messages.json"
        }
      },
      "model": {
        "key": "id_str",
        "fields": {
          "coordinates": {
            "type": "embedded",
            "fields": {
              "coordinates": {
                "type": "number",
                "many": true
              }
            }
          },
          "id_str": "string",
          "entities": {
            "type": "embedded",
            "fields": {
              "hashtags": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "text": "string"
                }
              }
            }
          },
          "created_at": "string",
          "text": "string",
          "sender": {
            "type": "embedded",
            "fields": {
              "id_str": "string",
              "profile_image_url_https": "string",
              "screen_name": "string",
              "name": "string"
            }
          },
          "extended_entities": {
            "type": "embedded",
            "fields": {
              "media": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "id_str": "string",
                  "expanded_url": "string",
                  "type": "string",
                  "media_url_https": "string",
                  "video_info": {
                    "type": "embedded",
                    "fields": {
                      "variants": {
                        "type": "embedded",
                        "many": true,
                        "fields": {
                          "content_type": "string",
                          "bitrate": "integer",
                          "url": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "DirectMessagesSent": {
      "method": "GET",
      "parameters": {
        "count": {
          "description": "Pagination counts",
          "value": 200
        },
        "since_id": {
          "description": "Most recent direct message that was saved in BitScoop",
          "value": "$connection.endpoint_data.DirectMessagesSent.since_id"
        },
        "max_id": {
          "description": "Highest ID of next page call",
          "value": "$connection.endpoint_data.DirectMessagesSent.max_id"
        }
      },
      "routes": {
        "single": null,
        "collection": {
          "data": "",
          "path": "1.1/direct_messages/sent.json"
        }
      },
      "model": {
        "key": "id_str",
        "fields": {
          "coordinates": {
            "type": "embedded",
            "fields": {
              "coordinates": {
                "type": "number",
                "many": true
              }
            }
          },
          "id_str": "string",
          "entities": {
            "type": "embedded",
            "fields": {
              "hashtags": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "text": "string"
                }
              }
            }
          },
          "created_at": "string",
          "text": "string",
          "recipient": {
            "type": "embedded",
            "fields": {
              "id_str": "string",
              "profile_image_url_https": "string",
              "screen_name": "string",
              "name": "string"
            }
          },
          "extended_entities": {
            "type": "embedded",
            "fields": {
              "media": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "id_str": "string",
                  "expanded_url": "string",
                  "type": "string",
                  "media_url_https": "string",
                  "video_info": {
                    "type": "embedded",
                    "fields": {
                      "variants": {
                        "type": "embedded",
                        "many": true,
                        "fields": {
                          "content_type": "string",
                          "bitrate": "integer",
                          "url": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "Tweets": {
      "method": "GET",
      "parameters": {
        "count": {
          "description": "Pagination counts",
          "value": 200
        },
        "since_id": {
          "description": "Most recent tweet that was saved in BitScoop",
          "value": "$connection.endpoint_data.Tweets.since_id"
        },
        "max_id": {
          "description": "Highest ID of next page call",
          "value": "$connection.endpoint_data.Tweets.max_id"
        }
      },
      "routes": {
        "single": null,
        "collection": {
          "data": "",
          "path": "1.1/statuses/user_timeline.json"
        }
      },
      "model": {
        "key": "name",
        "fields": {
          "coordinates": {
            "type": "embedded",
            "fields": {
              "coordinates": {
                "type": "number",
                "many": true
              }
            }
          },
          "id_str": "string",
          "created_at": "string",
          "text": "string",
          "entities": {
            "type": "embedded",
            "fields": {
              "hashtags": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "text": "string"
                }
              }
            }
          },
          "in_reply_to_user_id_str": "string",
          "in_reply_to_screen_name": "string",
          "extended_entities": {
            "type": "embedded",
            "fields": {
              "media": {
                "type": "embedded",
                "many": true,
                "fields": {
                  "id_str": "string",
                  "expanded_url": "string",
                  "type": "string",
                  "media_url_https": "string",
                  "video_info": {
                    "type": "embedded",
                    "fields": {
                      "variants": {
                        "type": "embedded",
                        "many": true,
                        "fields": {
                          "content_type": "string",
                          "bitrate": "integer",
                          "url": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "user": {
            "type": "related",
            "ref": "User",
            "reverse": {
              "parameters": {
                "user_id": "in_reply_to_user_id_str"
              }
            }
          }
        }
      }
    },
    "User": {
      "method": "GET",
      "except": [404],
      "parameters": {
        "user_id": {
          "description": "The user's ID",
          "supplied": true
        },
        "include_entities": {
          "description": "Whether or not to include the user's last tweet",
          "value": false
        }
      },
      "routes": {
        "single": null,
        "collection": {
          "data": "",
          "path": "1.1/users/lookup.json"
        }
      },
      "model": {
        "key": "id_str",
        "fields": {
          "profile_image_url_https": "string",
          "name": "str"
        }
      }
    }
  }
}
